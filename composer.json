{
  "name": "dravencms/tag",
  "type": "dravencms-package",
  "description": "Tag module for dravencms",
  "keywords": [
    "dravencms",
    "tag"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/locale": ">=v1.1.0",
    "dravencms/admin": ">=2.0",
    "nette/di": ">=3.0"
  },
  "suggest": {
    "dravencms/structure": "Install dravencms/structure module for frontend tag components to work"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": "dravencms.config.neon"
    }
  }
}
